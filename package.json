{
	"name": "sv-router",
	"version": "0.8.0",
	"description": "Modern Svelte Routing",
	"keywords": [
		"svelte",
		"router",
		"spa"
	],
	"homepage": "https://github.com/colinlienard/sv-router",
	"bugs": {
		"url": "https://github.com/colinlienard/sv-router/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/colinlienard/sv-router.git"
	},
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"import": "./src/index.js",
			"svelte": "./src/index.js",
			"types": "./src/index.d.ts"
		},
		"./vite-plugin": {
			"import": "./src/vite-plugin/index.js",
			"types": "./src/vite-plugin/index.d.ts"
		}
	},
	"bin": "./src/cli/index.js",
	"files": [
		"src"
	],
	"scripts": {
		"create:dev": "pnpm --filter create-sv-router dev",
		"create:build": "pnpm --filter create-sv-router build",
		"docs:dev": "pnpm --filter docs dev",
		"docs:build": "pnpm --filter docs build",
		"docs:preview": "pnpm --filter docs preview",
		"test": "vitest",
		"check": "svelte-check && pnpm -r check",
		"lint": "eslint . --max-warnings 0",
		"lint:fix": "eslint . --fix",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"changeset": "changeset"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"esm-env": "^1.2.2"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.4",
		"@eslint/js": "^9.28.0",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@types/node": "^22.15.30",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-svelte": "^3.9.1",
		"eslint-plugin-unicorn": "^59.0.1",
		"globals": "^16.2.0",
		"jsdom": "^26.1.0",
		"prettier": "^3.5.3",
		"prettier-plugin-jsdoc": "^1.3.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte-check": "^4.2.1",
		"type-testing": "^0.2.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.1",
		"vite": "^7.0.0",
		"vitest": "^3.2.2"
	},
	"peerDependencies": {
		"svelte": "^5"
	},
	"packageManager": "pnpm@10.11.1"
}
